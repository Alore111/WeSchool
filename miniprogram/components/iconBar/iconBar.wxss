.scroll1 {
  border-radius: 20rpx;
  width: 710rpx;
  height: 320rpx;
  background-color: rgba(255,255,255,0.7);
}
.scroll {
  display: flex;
  flex-direction: row;
  border-radius: 20rpx;
  /* width: 730rpx; */
  height: 300rpx;
  /* margin: 10rpx; */
}

.scroll_view {
  display: flex;
  /* background-color: #fff; */
  height: 300rpx;
  border-radius: 20rpx;
}

.iconn {
  /* background-color: #fff; */
  display: flex;
  flex-wrap: wrap;
  width: 730rpx;
  height: 300rpx;
}

.iconn_item {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  /* width: 25%; */
  width: 141rpx;
  padding: 10rpx 0 20rpx 0;
  height: calc(150rpx - 30rpx);
}

.iconn_item_url {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  min-width: 120rpx;
}

.iconn_item_url image {
  width: 80rpx;
  height: 80rpx;
  display: block;
}

.iconn_item_url text {
  display: block;
  color: #888;
  margin-top: 10rpx;
  line-height: 30rpx;
  font-size: 26rpx;
  text-align: center;
}
.slide {
  display: flex;
  justify-content: center;
  align-items: center;
  background: #fff;
  width: 100%;
}

.slide-bar {
  position: relative;
  width: 90rpx;
  height: 9rpx;
  border-radius: 20rpx;
  background-color: #eee;
}

.slide-show {
  position: relative;
  height: 100%;
  border-radius: 20rpx;
  background-color: rgb(236, 94, 47);
}
@media (prefers-color-scheme: dark) {
  .scroll1{
    background-color: #f5f4f4;
  }
  .scroll1 image{
    filter: invert(0%) !important;
  }
}