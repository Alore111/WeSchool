<view class="wrap">
    <view class="img">
        <image src="/images/Camera-3.png" hidden="{{false}}"/>
            <swiper indicator-dots='true' indicator-color="#fff" indicator-active-color='red' current="{{current}}" hidden="{{true}}">
                <block wx:for="{{photo}}" wx:key="photo">
                    <swiper-item >
                        <image data-src="{{item}}" src="{{item}}" mode="aspectFill" style="z-index: 11;width:600rpx;height:100%"/>
                    </swiper-item>
                </block>
            </swiper>
    </view>
</view>