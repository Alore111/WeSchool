/**index.wxss**/

.slider {
    width: 650rpx;
    margin: 40rpx auto;
}

.task_text {
    height: 120rpx;
    margin: 40rpx auto;
    text-align: center;
}

.task_text .task_title {
    font-size: 35rpx;
    height: 70rpx;
    line-height: 70rpx;
}

.task_text .task_desc {
    font-size: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #999999;
}

.task_cate {
    width: 660rpx;
    margin: 0 auto;
    display: flex;
    flex-wrap: wrap;
}

.task_cate .cate_item {
    width: 220rpx;
    height: 130rpx;
    text-align: center;
    margin-bottom: 50rpx;
}

.task_cate .cate_item .cate_icon {
    height: 70rpx;
}

.task_cate .cate_item .cate_icon image {
    width: 50rpx;
    height: 50rpx;
}

.task_cate .cate_item .cate_text {
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
}

.task_cate .cate_item .cate_text_active {
    color: #E7624F;
}

.start {
    width: 280rpx;
    height: 90rpx;
    line-height: 90rpx;
    text-align: center;
    margin: 40rpx auto;
    border: 2rpx solid #E7624F;
    color: #E7624F;
    border-radius: 20rpx;
}

.clock {
    overflow: hidden;
    /* background: #E7624F; */
    background-image: url('https://636c-cloud1-6gtqj1v4873bad50-1307814679.tcb.qcloud.la/tomato_daka/time_bg.jpg?sign=dfd427ce07bca11ea72e4cbfb8e74f31&t=1647312070');
    background-repeat: no-repeat;
    background-size: 100% 100%;
    position: relative;
}
.progress_water_left{
  width: 70rpx;
  height: 70rpx;
  position: absolute;
  left: 100rpx;
  top: 120rpx;
}
.progress_water_right{
  width: 70rpx;
  height: 70rpx;
  position: absolute;
  right: 100rpx;
  top: 200rpx;
}
.progress_water_center{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 260rpx;
  left: 320rpx;
}
.progress {
    width: 400rpx;
    height: 400rpx;
    /* background: orange; */
    margin: 400rpx auto 0 auto;
    position: relative;
    /* top: 300rpx; */
}

.progress .progress_bg,
.progress .progress_active {
    position: absolute;
    left: 0;
    top: 0;
    width: 400rpx;
    height: 400rpx;
}

.progress .progress_text {
    width: 160rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 30rpx;
    color: #96cddd;
    text-align: center;
    position: absolute;
    left: 120rpx;
    top: 170rpx;
    font-size: 50rpx;
}

.btns .okBtn,
.btns .pauseBtn,
.btns .continueBtn,
.btns .cancelBtn {
    width: 280rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    color: #ffffff;
    border: 3rpx solid #ffffff;
    border-radius: 20rpx;
    margin: 100rpx auto 20rpx auto;
}
