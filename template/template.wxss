/* template/template.wxss */

.navigate_item{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  line-height: 100rpx;
}
.navigate_item image:first-child{
  width: 50rpx;
  height: 50rpx;
  padding: 0 20rpx;
}
.navigate_item text{
  flex: 1;
  text-align: start;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.navigate_item image:last-child{
  width: 30rpx;
  height: 30rpx;
  padding: 0 20rpx;
}